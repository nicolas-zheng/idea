<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Desktop Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoDesktop</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Desktop Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Home Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoHome</IdeaActionID>
          <ShortCut>'Ctrl+1'</ShortCut>
          <Description>Home Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram Popup...">
        <Item>
          <IdeaActionID>ShowUmlDiagramPopup</IdeaActionID>
          <ShortCut>'Ctrl+Alt+U'</ShortCut>
          <Description>Show Diagram Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>